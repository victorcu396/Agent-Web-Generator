# Google API Keys
GOOGLE_API_KEY=your_google_api_key_here
STITCH_API_KEY=your_stitch_mcp_key_here

# Database Configuration
# Formato: postgresql+psycopg2://usuario:contraseña@host:puerto/nombre_db
DATABASE_URL=postgresql+psycopg2://postgres:postgres@127.0.0.1:5432/postgres

# Opcional: Configuración adicional de PostgreSQL
# DB_HOST=127.0.0.1
# DB_PORT=5432
# DB_NAME=postgres
# DB_USER=postgres
# DB_PASSWORD=postgres

# Application Settings
ENV=development
DEBUG=True
