           list-style: none;\n            margin: 0;\n            padding: 0;\n            display: flex;\n            gap: 2rem;\n        }\n\n        .nav-links a {\n            text-decoration: none;\n            color: var(--text-color-dark);\n            font-weight: 500;\n            transition: color 0.3s ease;\n        }\n\n        .nav-links a:hover {\n            color: var(--primary-color);\n        }\n\n        .btn {\n            display: inline-block;\n            padding: 0.75rem 1.5rem;\n            border-radius: 0.375rem;\n            text-decoration: none;\n            font-weight: 600;\n            transition: background-color 0.3s ease, color 0.3s ease;\n            border: none;\n            cursor: pointer;\n        }\n\n        .btn-primary {\n            background-color: var(--primary-color);\n            color: var(--bg-white);\n        }\n\n        .btn-primary:hover {\n            background-color: var(--secondary-color);\n        }\n\n        .btn-secondary {\n            background-color: transparent;\n            color: var(--primary-color);\n            border: 1px solid var(--primary-color);\n        }\n\n        .btn-secondary:hover {\n            background-color: var(--primary-color);\n            color: var(--bg-white);\n        }\n\n        /* Sections */\n        section {\n            padding: 6rem 0;\n            text-align: center;\n        }\n\n        section h2 {\n            font-size: 2.5rem;\n            color: var(--text-color-dark);\n            margin-bottom: 1rem;\n            font-weight: 700;\n        }\n\n        section p {\n            font-size: 1.15rem;\n            color: var(--text-color-light);\n            margin-bottom: 2rem;\n            max-width: 700px;\n            margin-left: auto;\n            margin-right: auto;\n        }\n\n        /* Hero Section */\n        #hero {\n            background: linear-gradient(135deg, var(--bg-white) 0%, var(--bg-light) 100%);\n            padding: 8rem 0;\n        }\n\n        #hero h1 {\n            font-size: 3.5rem;\n            color: var(--text-color-dark);\n            margin-bottom: 1.5rem;\n            font-weight: 800;\n            line-height: 1.1;\n        }\n\n        #hero .hero-buttons {\n            display: flex;\n            justify-content: center;\n            gap: 1.5rem;\n            margin-top: 3rem;\n        }\n\n        /* Features Section */\n        #features {\n            background-color: var(--bg-white);\n            border-bottom: 1px solid var(--border-color);\n        }\n\n        .features-grid {\n            display: grid;\n            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n            gap: 2rem;\n            margin-top: 3rem;\n            text-align: left;\n        }\n\n        .feature-item {\n            background-color: var(--bg-white);\n            padding: 2rem;\n            border-radius: 0.5rem;\n            box-shadow: var(--shadow-md);\n            transition: transform 0.3s ease, box-shadow 0.3s ease;\n            border: 1px solid var(--border-color);\n        }\n\n        .feature-item:hover {\n            transform: translateY(-5px);\n            box-shadow: 0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05);\n        }\n\n        .feature-item h3 {\n            font-size: 1.5rem;\n            color: var(--primary-color);\n            margin-bottom: 1rem;\n            font-weight: 600;\n        }\n\n        .feature-item p {\n            font-size: 1rem;\n            color: var(--text-color-light);\n            margin-bottom: 0;\n        }\n\n        /* Pricing Section */\n        #pricing {\n            background-color: var(--bg-light);\n            border-bottom: 1px solid var(--border-color);\n        }\n\n        .pricing-grid {\n            display: grid;\n            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n            gap: 2rem;\n            margin-top: 3rem;\n        }\n\n        .pricing-card {\n   